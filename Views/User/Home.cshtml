    <section class="single_product_list">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                @foreach (var product in Model.Take(3)) 
                {
                    var cartItem = userId != null ? _context.Carts.FirstOrDefault(c => c.UserId == userId && c.ProductId == product.ProductID) : null;

                    <div class="single_product_iner">
                        <div class="row align-items-center justify-content-between">
                            <div class="col-lg-6 col-sm-6">
                                <div class="single_product_img">
                                    <img src="@product.ImagePath" class="img-fluid" alt="@product.ProductName">
                                    <img src="~/pillowmart-master/img/product_overlay.png" alt="#" class="product_overlay img-fluid">
                                </div>
                            </div>
                            <div class="col-lg-5 col-sm-6">
                                <div class="single_product_content">
                                    <h5>Starting from ₹@product.Price</h5>
                                    <h2>
                                        <a asp-action="Details" asp-controller="UserProducts" asp-route-id="@product.ProductID">
                                            @product.ProductName
                                        </a>
                                    </h2>
                                    <p>@product.Description</p>

                                    @if (cartItem == null)
                                    {
                                        <form asp-action="AddToCart" asp-controller="Cart" method="post">
                                            <input type="hidden" name="productId" value="@product.ProductID" />
                                            <label>Quantity:</label>
                                            <select name="quantity" class="form-select d-inline w-auto">
                                                @for (int i = 1; i <= 10; i++)
                                                {
                                                    <option value="@i">@i</option>
                                                }
                                            </select>
                                            <button type="submit" class="btn btn-primary mt-2">
                                                <i class="fas fa-cart-plus"></i> Add to Cart
                                            </button>
                                        </form>
                                    }
                                    else
                                    {
                                        <a asp-action="Index" asp-controller="Cart" class="btn btn-success mt-2">
                                            <i class="fas fa-shopping-cart"></i> Go to Cart
                                        </a>
                                    }

                                    <a asp-action="AddToWishlist" asp-controller="Wishlist" asp-route-productId="@product.ProductID" class="btn btn-danger mt-2">
                                        <i class="fas fa-heart"></i> Add to Wishlist
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                }

                <!-- View More Button -->
                    <div class="text-center mt-4">
                        <a asp-action="Product_list" asp-controller="UserProducts"
                           class="btn btn-lg px-5 py-2 fw-semibold text-white"
                           style="background: linear-gradient(to right, #4facfe 0%, #00f2fe 100%);
