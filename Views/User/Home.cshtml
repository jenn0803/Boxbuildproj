    <section class="banner_part">
        <div class="banner_wrapper">
            <!-- Lavender Box Left -->
            <div class="lavender-box"></div>

            <!-- Right Side: Slider + Text -->
            <div class="slider-box">
                <div class="banner_slider">
                    <img src="~/pillowmart-master/img/about_img_1.png" class="img-fluid banner_img active" alt="Banner 1" />
                    <img src="~/pillowmart-master/img/about_img_2.png" class="img-fluid banner_img d-none" alt="Banner 2" />
                    <img src="~/pillowmart-master/img/baner.jpg" class="img-fluid banner_img d-none" alt="Banner 3" />
                </div>

                <div class="slider_text">
                    <h4 id="banner-heading">"Box It Right!"</h4>
                    <p id="banner-subtext">"More Than Just a Box – It’s Protection for Your Product!"</p>
                    <a class="btn_1 mt-2" asp-controller="User" asp-action="Product_list">Shop Now!</a>
                </div>
            </div>
        </div>
    </section>

    <style>
        .banner_part {
            width: 100%;
            height: 100vh;
            overflow: hidden;
        }

        .banner_wrapper {
            display: flex;
            width: 100%;
            height: 100%;
        }

        .lavender-box {
            width: 25%;
            background-color: #c8a2c8; /* Lavender color */
        }

        .slider-box {
            width: 75%;
            position: relative;
            overflow: hidden;
            display: flex;
            flex-direction: column;
            justify-content: flex-end;
        }

        .banner_slider {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }

        .banner_img {
            position: absolute;
            width: 100%;
            height: 100%;
            object-fit: cover;
            opacity: 0;
            transition: opacity 0.8s ease-in-out;
            z-index: 1;
        }

            .banner_img.active {
                opacity: 1;
                z-index: 2;
            }

        .slider_text {
            position: relative;
            z-index: 3;
            padding: 40px;
            background: rgba(0, 0, 0, 0.4);
            color: white;
            text-align: center;
        }

        .btn_1 {
            background-color: #ff7f50;
            color: #fff;
            padding: 10px 25px;
            border-radius: 30px;
            border: none;
            text-decoration: none;
            display: inline-block;
            transition: background-color 0.3s ease;
        }

            .btn_1:hover {
                background-color: #ff6333;
            }

        /* Responsive Design */
        @@media (max-width: 768px) {
            .banner_wrapper

        {
            flex-direction: column;
        }

        .lavender-box,
        .slider-box {
            width: 100%;
            height: 50vh;
        }

        .slider_text {
            padding: 20px;
        }

        }
    </style>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const images = document.querySelectorAll('.banner_img');
            const headings = [
                `"Box It Right!"`,
                `"Wrap with Confidence!"`,
                `"Secure. Ship. Smile!"`
            ];
            const subtexts = [
                `"More Than Just a Box – It’s Protection for Your Product!"`,
                `"We pack with purpose and pride."`,
                `"Delivering protection in every fold."`
            ];

            let index = 0;

            function showNextSlide() {
                images[index].classList.remove('active');
                images[index].classList.add('d-none');

                index = (index + 1) % images.length;

                images[index].classList.remove('d-none');
                images[index].classList.add('active');

                document.getElementById('banner-heading').innerHTML = headings[index];
                document.getElementById('banner-subtext').innerHTML = subtexts[index];
            }

            setInterval(showNextSlide, 4000);
        });
    </script>






